
@{
    ViewBag.Title = "AngularExample";
}
<script src="~/Scripts/angular.min.js"></script>
<h2>AngularExample</h2>
<script src="~/Scripts/MyController.js"></script>
<div ng-app="myApp" ng-init="name='prasad'">
    {{5+5}}-<p>{{name}}</p><br />
    <span ng-bind="name"></span><br />
    <input type="text" ng-model="fname" /><br />
    {{fname}}
    <input type="text" /><br />
    <div ng-controller="myCtrl">
     
        <div>
            <table class="table-bordered">
                <tr>
                    <th>EmpId</th>
                    <th>EmpName</th>
                    <th>EmpSalary</th>
                </tr>
                <tr ng-repeat="x in EmployeeData">
                    <td>{{x.EmpId}}</td>
                    <td>{{x.EmpName}}</td>
                    <td>{{x.EmpSalary}}</td>
                </tr>
            </table>
        </div>

        <form>
            <label>EmpName</label>
            <input type="text" ng-model="EmpName" /><br />
            <label>EmpSalary</label>
            <input type="text" ng-model="EmpSalary" /><br />
            <input type="button" value="Save" ng-click="save()" />
        </form>

    </div>

</div>

 